{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@react-buoy/shared-ui": [
        "./node_modules/@react-buoy/core/node_modules/@react-buoy/shared-ui/lib/typescript/module/index.d.ts"
      ],
      "@react-buoy/shared-ui/*": [
        "./node_modules/@react-buoy/core/node_modules/@react-buoy/shared-ui/lib/typescript/module/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
